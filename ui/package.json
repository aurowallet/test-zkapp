    "coi-serviceworker": "^0.1.7",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "next": "14.2.3",
    "o1js": "2.4.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "^6.0.8",
    "typescript": "5.4.5",
    "mina-attestations":"0.5.0",
    "react-hot-toast":"2.5.2",
    "@walletconnect/sign-client":"^2.19.1",
    "@web3modal/standalone":"2.4.3"
  },
