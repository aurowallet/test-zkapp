    "@types/react-dom": "18.2.12",
    "coi-serviceworker": "^0.1.7",
    "next": "13.5.4",
    "o1js": "0.18.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^6.0.8",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "2.1.4"
  }
}
